{
  "name": "cruisebound",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "typecheck": "turbo run typecheck",
    "prepare": "husky install"
  },
  "devDependencies": {
    "turbo": "^1.10.1",
    "husky": "^8.0.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "husky": "^8.0.3",
    "prettier": "^2.8.8"
  },
  "packageManager": "pnpm@7.25.0"
}
